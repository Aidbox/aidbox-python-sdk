APP_ID=app-py-example
APP_SECRET=secret
APP_URL=http://app:8081
APP_PORT=8081
AIO_PORT=8081
AIO_HOST=0.0.0.0
AIO_APP_PATH=.

OPENID_RSA=/var/config/jwtRS256.key
OPENID_RSA_PUB=/var/config/jwtRS256.key.pub
